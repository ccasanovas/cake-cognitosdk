{
    "name": "ccasanovas/cake-cognito",
    "description": "CognitoSDK plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "4.*",
        "aws/aws-sdk-php": "^3.54",
        "lcobucci/jwt": "^3.2",
        "ccasanovas/cake-apigateway": "2.3.*",
        "evil-corp/cakephp-awss3upload": "1.0.*",
        "muffin/footprint": "^1.2",
        "friendsofcake/search": "^3.1",
        "ralouphie/mimey": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Ccasanovas\\CognitoSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ccasanovas\\CognitoSDK\\Test\\": "tests",
            "Ccasanovas\\CognitoSDK\\Test\\Fixture\\": "tests"
        }
    }
}
