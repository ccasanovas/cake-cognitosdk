{
    "name": "ccasanovas/cake-cognito",
    "description": "CognitoSDK plugin for CakePHP",
    "type": "composer-plugin",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "cakephp/cakephp": "4.*",
        "aws/aws-sdk-php": "^3.54",
        "lcobucci/jwt": "^3.2",
        "ccasanovas/cakeapigateway": "1.*",
        "ccasanovas/cake-s3upload": "1.*",
        "muffin/footprint": "^3.0",
        "friendsofcake/search": "^6.2",
        "ralouphie/mimey": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ccasanovas\\CognitoSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ccasanovas\\CognitoSDK\\Test\\": "tests",
            "Ccasanovas\\CognitoSDK\\Test\\Fixture\\": "tests"
        }
    }
}
